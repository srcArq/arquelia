<span class="text-lg font-medium tracking-wide">
  Enjoying <span id="typed-effect" class="text-accent tracking-wide typed-cursor blinking"></span>
</span> 
<script >
  import Typed from 'typed.js';

  document.addEventListener('DOMContentLoaded', () => {
    const options = {
      strings: ['Frontend', 'Backend', 'Web', 'Mobile', 'Desktop', 'Fullstack', 'Learning'],
      typeSpeed: 100,
      backSpeed: 50,
      backDelay: 2000,
      loop: true,
    };

    new Typed('#typed-effect', options);
  });
</script>
